{
  "server": {
    "host": "localhost",
    "port": 8888,
    "file_log": true,
    "log_level": "info"
  },
  "web": {
    "path": "/",
    "dist_path": "dist",
    "spa_support": true
  },
  "websocket_proxy": {
    "path": "/ws",
    "forward_to": "ws://127.0.0.1:8000/ws",
    "timeout": 1000
  },
  "tcp_proxy": {
    "path": "/tcp",
    "forward_to": "127.0.0.1:8080",
    "timeout": 1000
  },
  "reverse_proxy": {
    "path": "/proxy",
    "forward_to": "http://localhost:5173",
    "timeout": 1000
  }
}
